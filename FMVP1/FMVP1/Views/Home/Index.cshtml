@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Danmeet Singh</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>HTML Danmeet </h2>
        <table border="1">
        <tr><td>Name </td><td>@Html.TextBox("Danmeet")</td></tr>
        <tr><td>Course </td><td>@Html.TextArea("Cpct")</td></tr>
        <tr><td>Language </td><td>@Html.CheckBox("Punjabi")</td><td> English @Html.CheckBox("Punjabi")</td></tr>
         <tr><td>Gender </td><td>@Html.RadioButton("Gender", "Male")</td><td> Female @Html.RadioButton("Gender", "Female" )</td></tr>
        </table>

    </div>
</div>